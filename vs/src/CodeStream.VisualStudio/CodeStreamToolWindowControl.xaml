<UserControl x:Class="CodeStream.VisualStudio.CodeStreamToolWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:vsshell="clr-namespace:Microsoft.VisualStudio.Shell;assembly=Microsoft.VisualStudio.Shell.15.0"
             xmlns:wpf="clr-namespace:DotNetBrowser.WPF;assembly=DotNetBrowser32"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Name="MyToolWindow">
    <Grid Name="grid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <wpf:WPFBrowserView Name="Browser" Grid.Column="0" FinishLoadingFrameEvent="Browser_FinishLoadingFrameEvent" Initialized="Browser_Initialized"/>
       
    </Grid>
 
</UserControl>
