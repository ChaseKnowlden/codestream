{
	"compilerOptions": {
		"baseUrl": "./",
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react",
		"module": "esnext",
		"moduleResolution": "node",
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": false,
		"noImplicitAny": false,
		"noUnusedLocals": false,
		"outDir": "dist",
		"paths": {
			"@codestream/protocols/agent": ["protocols/agent/agent.protocol"],
			"@codestream/protocols/api": ["protocols/agent/api.protocol"]
		},
		"rootDir": "./",
		"sourceMap": true,
		"strict": true,
		"target": "esnext"
	},
	"exclude": ["node_modules"]
}
