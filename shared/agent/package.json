{
    "name": "codestream-lsp-agent",
    "displayName": "CodeStream Agent",
    "description": "The knowledge base behind your codebase",
    "version": "0.0.1",
    "author": "CodeStream",
    "publisher": "CodeStream",
    "private": true,
    "license": "UNLICENSED",
    "homepage": "https://codestream.com",
    "bugs": {
        "url": "https://github.com/TeamCodeStream/codestream-lsp-agent"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TeamCodeStream/codestream-lsp-agent"
    },
    "engines": {
        "node": "*"
    },
    "main": "./dist/agent",
    "scripts": {
        "build": "npm run lint && webpack --env.development",
        "bundle": "npm run lint && webpack --env.production",
        "clean": "git clean -Xdf -e !node_modules -e !node_modules/**/*",
        "lint": "tslint --project tsconfig.json",
        "pack": "npm run clean && npm install --no-save && npm run bundle",
        "rebuild": "npm run clean && npm install --no-save && npm run build",
        "watch": "webpack --watch --env.development",
        "test": "mocha --compilerOptions --recursive --require node_modules/ts-node/register test/**/*.spec.ts"
    },
    "dependencies": {
        "date-fns": "^1.29.0",
        "diff": "3.5.0",
        "lodash.debounce": "4.0.8",
        "node-fetch": "2.2.0",
        "pubnub": "4.21.2",
        "reflect-metadata": "0.1.12",
        "string-similarity": "^1.2.0",
        "tmp": "0.0.33",
        "tslib": "1.9.3",
        "vscode-languageserver": "^4.4.0",
        "vscode-uri": "1.0.5"
    },
    "devDependencies": {
        "@types/chai": "4.1.4",
        "@types/diff": "3.5.1",
        "@types/mocha": "5.2.5",
        "@types/node": "7.0.67",
        "@types/node-fetch": "2.1.2",
        "@types/pubnub": "4.0.2",
        "@types/randomstring": "^1.1.6",
        "@types/string-similarity": "^1.2.0",
        "@types/tmp": "0.0.33",
        "chai": "4.1.2",
        "clean-webpack-plugin": "0.1.19",
        "filemanager-webpack-plugin": "1.0.27",
        "mocha": "5.2.0",
        "prettier": "1.13.7",
        "randomstring": "1.1.5",
        "ts-loader": "4.4.2",
        "ts-node": "7.0.0",
        "tslint": "5.11.0",
        "typescript": "2.9.2",
        "webpack": "4.16.2",
        "webpack-cli": "3.1.0",
        "webpack-node-externals": "1.7.2"
    }
}
