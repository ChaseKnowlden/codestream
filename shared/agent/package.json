    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TeamCodeStream/codestream-lsp-agent"
    },
    "engines": {
        "node": "*"
    },
    "main": "./dist/agent",
    "scripts": {
        "build": "webpack --env.development",
        "bundle": "npm run reset && webpack --env.production",
        "clean": "git clean -Xdf -e !node_modules -e !node_modules/**/*",
        "lint": "tslint --project tsconfig.json",
        "pretty": "prettier --config .prettierrc --write \"./src/**/*.ts\" ./package.json ./.prettierrc",
        "rebuild": "npm run reset && npm run build",
        "reset": "npm run clean && npm install --no-save",
        "test": "cross-env TS_NODE_COMPILER_OPTIONS=\"{ \\\"module\\\": \\\"commonjs\\\", \\\"types\\\": [\\\"node\\\"] }\" mocha --require ts-node/register --recursive \"src/test/**/*.spec.ts\"",
        "watch": "webpack --watch --env.development"
    },
    "dependencies": {
        "date-fns": "1.29.0",
        "diff": "3.5.0",
        "lodash.debounce": "4.0.8",
        "node-fetch": "2.2.0",
        "pubnub": "4.21.2",
        "reflect-metadata": "0.1.12",
        "string-similarity": "1.2.0",
        "tmp": "0.0.33",
        "tslib": "1.9.3",
        "vscode-languageserver": "4.4.0",
        "vscode-uri": "1.0.5"
    },
    "devDependencies": {
        "@types/chai": "4.1.4",
        "@types/diff": "3.5.1",
        "@types/mocha": "5.2.5",
        "@types/node": "7.0.67",
        "@types/node-fetch": "2.1.2",
        "@types/pubnub": "4.0.2",
        "@types/randomstring": "1.1.6",
        "@types/string-similarity": "1.2.0",
        "@types/tmp": "0.0.33",
        "chai": "4.1.2",
        "clean-webpack-plugin": "0.1.19",
        "cross-env": "5.2.0",
        "filemanager-webpack-plugin": "1.0.29",
        "ignore-loader": "0.1.2",
        "mocha": "5.2.0",
        "prettier": "1.14.0",
        "randomstring": "1.1.5",
        "ts-loader": "4.4.2",
        "ts-node": "7.0.0",
        "tslint": "5.11.0",
        "tslint-loader": "3.6.0",
        "typescript": "3.0.1",
        "webpack": "4.16.5",
        "webpack-cli": "3.1.0",
        "webpack-node-externals": "1.7.2"
    }
}
