{
    "name": "codestream-lsp-agent",
    "description": "The knowledge base behind your codebase",
    "version": "0.0.2",
    "author": "CodeStream",
    "publisher": "CodeStream",
    "private": true,
    "license": "UNLICENSED",
    "homepage": "https://codestream.com",
    "bugs": {
        "url": "https://github.com/TeamCodeStream/codestream-lsp-agent/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TeamCodeStream/codestream-lsp-agent"
    },
    "engines": {
        "node": "*"
    },
    "main": "./dist/agent",
    "scripts": {
        "build": "webpack --env.development",
        "bundle": "npm install --no-save && webpack --env.production",
        "bundle:ci": "npm run clean:ci && npm run bundle",
        "clean": "git clean -Xdf -e !node_modules -e !node_modules/**/*",
        "clean:ci": "git clean -xdf",
        "lint": "tslint --project tsconfig.json",
        "pretty": "prettier --config .prettierrc --write \"./src/**/*.ts\" ./package.json ./.prettierrc",
        "rebuild": "npm run clean && npm install --no-save && npm run build",
        "watch": "webpack --watch --env.development --info-verbosity verbose",
        "test-unit": "cross-env TS_NODE_COMPILER_OPTIONS=\"{ \\\"module\\\": \\\"commonjs\\\", \\\"types\\\": [\\\"node\\\"] }\" mocha --require ts-node/register --recursive \"test/unit/**/*.spec.ts\"",
        "test-unit-ci": "cross-env TS_NODE_COMPILER_OPTIONS=\"{ \\\"module\\\": \\\"commonjs\\\", \\\"types\\\": [\\\"node\\\"] }\" mocha --require ts-node/register --reporter mocha-teamcity-reporter --recursive \"test/unit/**/*.spec.ts\"",
        "test-acceptance": "cross-env TS_NODE_COMPILER_OPTIONS=\"{ \\\"module\\\": \\\"commonjs\\\", \\\"types\\\": [\\\"node\\\"] }\" mocha -r ts-node/register -r esm --recursive \"test/acceptance/**/*.spec.ts\"",
        "bash-test-acceptance": "export TS_NODE_COMPILER_OPTIONS=`cat tsnode-options.json` && mocha -r ts-node/register -r esm --recursive \"test/acceptance/**/*.spec.ts\"",
        "bash-test-acceptance-ci": "npm run bash-test-acceptance -- --reporter mocha-teamcity-reporter",
        "test-integration": "cross-env TS_NODE_COMPILER_OPTIONS=\"{ \\\"module\\\": \\\"commonjs\\\", \\\"types\\\": [\\\"node\\\"] }\" mocha --require ts-node/register --recursive \"test/integration/**/*.spec.ts\"",
        "bash-test-integration": "export TS_NODE_COMPILER_OPTIONS=`cat tsnode-options.json` && mocha --require ts-node/register --recursive \"test/integration/**/*.spec.ts\"",
        "bash-test-integration-ci": "npm run bash-test-integration -- --reporter mocha-teamcity-reporter",
        "bash-test-unit": "export TS_NODE_COMPILER_OPTIONS=`cat tsnode-options.json` && mocha --require ts-node/register --recursive \"test/unit/**/*.spec.ts\"",
        "bash-test-unit-ci": "npm run bash-test-unit -- --reporter mocha-teamcity-reporter"
    },
    "dependencies": {
        "@sentry/node": "4.5.0",
        "date-fns": "1.30.1",
        "diff": "4.0.1",
        "https-proxy-agent": "2.2.1",
        "lodash-es": "4.17.11",
        "mixpanel": "^0.10.1",
        "node-fetch": "2.3.0",
        "opn": "5.4.0",
        "pubnub": "4.21.7",
        "string-similarity": "3.0.0",
        "tmp": "0.0.33",
        "tslib": "1.9.3",
        "vscode-languageserver": "5.2.1",
        "vscode-uri": "1.0.6",
        "write-file-atomic": "2.3.0"
    },
    "devDependencies": {
        "@sentry/types": "4.5.0",
        "@slack/client": "4.8.0",
        "@types/chai": "4.1.7",
        "@types/diff": "3.5.2",
        "@types/lodash-es": "4.17.1",
        "@types/mocha": "5.2.5",
        "@types/node": "8.10.39",
        "@types/node-fetch": "2.1.2",
        "@types/pubnub": "4.0.4",
        "@types/randomstring": "1.1.6",
        "@types/sinon": "5.0.5",
        "@types/sinon-chai": "3.2.0",
        "@types/string-similarity": "3.0.0",
        "@types/tmp": "0.0.33",
        "@types/write-file-atomic": "2.1.1",
        "chai": "4.2.0",
        "clean-webpack-plugin": "1.0.1",
        "cross-env": "5.2.0",
        "esm": "3.1.1",
        "filemanager-webpack-plugin": "2.0.5",
        "mocha": "5.2.0",
        "mocha-teamcity-reporter": "2.5.1",
        "prettier": "1.16.1",
        "prettier-tslint": "0.4.2",
        "randomstring": "1.1.5",
        "sanitize-filename": "^1.6.1",
        "sinon": "6.3.4",
        "sinon-chai": "3.2.0",
        "terser-webpack-plugin": "1.2.1",
        "ts-loader": "5.3.3",
        "ts-node": "7.0.1",
        "tslint": "5.12.1",
        "tslint-loader": "3.5.4",
        "typescript": "3.2.4",
        "webpack": "4.29.0",
        "webpack-cli": "3.2.1"
    }
}
