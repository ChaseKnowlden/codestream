{
	"name": "CodeStream",
	"main": "./dist/codestream",
	"version": "0.0.23",
	"description": "Build the knowledge base behind your codebase. CodeStream allows your development team to communicate within Atom and conversation threads automatically become annotations that live with your codebase forever.",
	"keywords": [
		"git",
		"team",
		"chat"
	],
	"activationHooks": [
		"core:loaded-shell-environment"
	],
	"repository": "https://github.com/TeamCodeStream/atom-client",
	"license": "",
	"engines": {
		"atom": ">=1.34.0 <2.0.0"
	},
	"scripts": {
		"prettify": "prettier --config .prettierrc --write ./{lib,spec}/*.js ./{lib,spec}/**/*.js ./package.json ./.prettierrc",
		"build": "webpack --env.development --info-verbosity verbose",
		"watch": "webpack --watch --env.development",
		"extension:build": "webpack --config-name extension --env.development --info-verbosity verbose",
		"extension:watch": "webpack --watch --config-name extension --env.development --info-verbosity verbose",
		"webview:build": "webpack --config-name webview --env.development --info-verbosity verbose",
		"webview:watch": "webpack --watch --config-name webview --env.development --info-verbosity verbose"
	},
	"deserializers": {
		"codestream/CodestreamView": "deserializeCodestreamView"
	},
	"consumedServices": {
		"status-bar": {
			"versions": {
				"^1.0.0": "consumeStatusBar"
			}
		}
	},
	"dependencies": {
		"@types/atom": "1.31.0",
		"@types/electron": "1.6.10",
		"@types/node-fetch": "^2.1.4",
		"@types/react": "16.8.1",
		"@types/react-dom": "16.0.11",
		"@types/uuid": "3.4.4",
		"atom-languageclient": "0.9.9",
		"emoji-mart": "2.9.2",
		"highlight.js": "9.15.5",
		"rc-tooltip": "3.7.3",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"uuid": "3.3.2"
	},
	"devDependencies": {
		"acorn": "6.0.7",
		"babel-core": "6.26.3",
		"babel-eslint": "^8.2.3",
		"babel-loader": "7.1.5",
		"babel-plugin-transform-class-properties": "6.24.1",
		"babel-plugin-transform-object-rest-spread": "6.26.0",
		"babel-preset-env": "1.7.0",
		"babel-preset-react": "6.24.1",
		"clean-webpack-plugin": "1.0.1",
		"filemanager-webpack-plugin": "2.0.3",
		"html-loader": "0.5.5",
		"html-webpack-plugin": "3.2.0",
		"ignore-loader": "0.1.2",
		"mini-css-extract-plugin": "0.5.0",
		"prettier": "1.16.4",
		"ts-loader": "5.3.3",
		"tsconfig-paths-webpack-plugin": "3.2.0",
		"tslint": "5.12.1",
		"tslint-loader": "3.5.4",
		"typescript": "3.3.1",
		"webpack": "4.29.5",
		"webpack-cli": "3.2.3",
		"webpack-node-externals": "1.7.2"
	}
}
