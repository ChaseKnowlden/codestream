{
	"name": "codestream",
	"main": "./dist/codestream",
	"version": "1.0.0",
	"description": "Discuss, review, and share code with your team. Integrates w/ Slack, Jira, Trello, and GitHub",
	"keywords": [
		"git",
		"team",
		"comment",
		"collaboration"
	],
	"activationHooks": [
		"core:loaded-shell-environment"
	],
	"repository": "https://github.com/TeamCodeStream/atom-client",
	"license": "",
	"engines": {
		"atom": ">=1.34.0 <2.0.0"
	},
	"deserializers": {
		"codestream/CodestreamView": "deserializeCodestreamView"
	},
	"providedServices": {
		"codestream-info": {
			"description": "Provides environment config info",
			"versions": {
				"1.0.0": "provideEnvironmentConfig"
			}
		}
	},
	"consumedServices": {
		"status-bar": {
			"versions": {
				"^1.0.0": "consumeStatusBar"
			}
		}
	},
	"configSchema": {
		"team": {
			"type": "string",
			"default": "",
			"description": "Specifies team name to sign into"
		},
		"avatars": {
			"type": "boolean",
			"default": true
		},
		"showMarkers": {
			"type": "boolean",
			"default": true
		},
		"autoHideMarkers": {
			"type": "boolean",
			"default": true
		},
		"autoSignIn": {
			"type": "boolean",
			"default": true,
			"description": "Automatically sign in when restoring a window with a previous session"
		},
		"traceLevel": {
			"type": "string",
			"default": "verbose",
			"enum": [
				"silent",
				"errors",
				"verbose",
				"debug"
			]
		},
		"proxySupport": {
			"type": "string",
			"default": "off",
			"enum": [
				"override",
				"on",
				"off"
			],
			"description": "Specifies whether to support proxy servers. If 'override', a url is required"
		},
		"proxyUrl": {
			"type": "string",
			"default": ""
		},
		"proxyStrictSSL": {
			"type": "boolean",
			"default": true
		}
	},
	"scripts": {
		"analyze:bundle": "webpack --env.analyzeBundle",
		"prettify": "prettier --config .prettierrc --write ./{lib,spec}/*.js ./{lib,spec}/**/*.js ./package.json ./.prettierrc",
		"bundle": "./bin/atom-bundle",
		"publish:dev": "./bin/atom-publish-dev",
		"build": "webpack --env.development",
		"watch": "webpack --watch --env.development",
		"extension:build": "webpack --config-name extension --env.development",
		"extension:watch": "webpack --watch --config-name extension --env.development",
		"webview:analyze": "webpack --env.analyzeBundleWebview",
		"webview:build": "webpack --config-name webview --env.development",
		"webview:watch": "webpack --watch --config-name webview --env.development"
	},
	"dependencies": {
		"atom-languageclient": "0.9.9",
		"fs-plus": "3.1.1",
		"uuid": "3.3.2"
	},
	"devDependencies": {
		"@babel/cli": "7.2.3",
		"@babel/core": "7.3.4",
		"@babel/plugin-proposal-class-properties": "7.3.4",
		"@babel/plugin-proposal-object-rest-spread": "7.3.4",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/plugin-transform-runtime": "7.3.4",
		"@babel/preset-env": "7.3.4",
		"@babel/preset-react": "7.0.0",
		"@babel/preset-typescript": "7.3.3",
		"@babel/runtime": "7.3.4",
		"@types/atom": "1.31.1",
		"@types/electron": "1.6.10",
		"@types/fs-plus": "^3.0.1",
		"@types/node-fetch": "2.1.6",
		"@types/uuid": "3.4.4",
		"acorn": "6.0.7",
		"babel-loader": "8.0.5",
		"clean-webpack-plugin": "1.0.1",
		"filemanager-webpack-plugin": "2.0.5",
		"fork-ts-checker-webpack-plugin": "1.0.0",
		"html-loader": "0.5.5",
		"html-webpack-plugin": "3.2.0",
		"ignore-loader": "0.1.2",
		"mini-css-extract-plugin": "0.5.0",
		"prettier": "1.16.4",
		"terser-webpack-plugin": "1.2.3",
		"ts-loader": "5.3.3",
		"tsconfig-paths-webpack-plugin": "3.2.0",
		"tslint": "5.13.1",
		"tslint-loader": "3.5.4",
		"typescript": "3.3.1",
		"webpack": "4.29.6",
		"webpack-bundle-analyzer": "3.1.0",
		"webpack-cli": "3.2.3"
	}
}
