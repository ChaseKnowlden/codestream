{
	"name": "CodeStream",
	"main": "./dist/codestream",
	"version": "0.0.23",
	"description": "Build the knowledge base behind your codebase. CodeStream allows your development team to communicate within Atom and conversation threads automatically become annotations that live with your codebase forever.",
	"keywords": [
		"git",
		"team",
		"chat"
	],
	"activationCommands": {},
	"repository": "https://github.com/TeamCodeStream/atom-client",
	"license": "",
	"engines": {
		"atom": ">=1.24.1 <2.0.0"
	},
	"scripts": {
		"prettify": "prettier --config .prettierrc --write ./{lib,spec}/*.js ./{lib,spec}/**/*.js ./package.json ./.prettierrc",
		"build": "webpack --env.development",
		"watch": "webpack --watch --env.development"
	},
	"deserializers": {
		"codestream/CodestreamView": "deserializeCodestreamView"
	},
	"consumedServices": {
		"status-bar": {
			"versions": {
				"^1.0.0": "consumeStatusBar"
			}
		}
	},
	"dependencies": {
		"@types/atom": "1.31.0",
		"@types/electron": "1.6.10",
		"@types/node-fetch": "^2.1.4",
		"@types/react": "16.8.1",
		"@types/react-dom": "16.0.11",
		"@types/uuid": "3.4.4",
		"atom-languageclient": "0.9.9",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"uuid": "3.3.2"
	},
	"devDependencies": {
		"@babel/core": "7.0.0",
		"@babel/plugin-proposal-class-properties": "7.0.0",
		"@babel/plugin-proposal-object-rest-spread": "7.0.0",
		"@babel/preset-env": "7.0.0",
		"@babel/preset-flow": "7.0.0",
		"@babel/preset-react": "7.0.0",
		"acorn": "6.0.7",
		"babel-eslint": "^8.2.3",
		"babel-loader": "8.0.0",
		"clean-webpack-plugin": "1.0.1",
		"filemanager-webpack-plugin": "2.0.3",
		"ignore-loader": "0.1.2",
		"prettier": "^1.14.2",
		"ts-loader": "5.3.3",
		"tslint": "5.12.1",
		"tslint-loader": "3.5.4",
		"typescript": "3.3.1",
		"webpack": "4.29.0",
		"webpack-cli": "3.2.1",
		"webpack-node-externals": "1.7.2"
	}
}
