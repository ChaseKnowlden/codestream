{
  "name": "CodeStream",
  "keywords": [
    "git",
    "team",
    "chat"
  ],
  "license": "",
  "scripts": {
    "build": "rollup -c",
    "build-watch": "rollup -c -w",
    "vscode-bundle": "rollup -c && npm run vscode-build-css",
    "vscode-bundle-watch": "rollup -c -w",
    "vscode-build-css": "lessc ./styles/vscode-stream.less ./../styles/stream.css",
    "prettify": "prettier --config .prettierrc --write ./{lib,spec}/*.js ./{lib,spec}/**/*.js ./package.json ./.prettierrc"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.44",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "codestream-components": "git+https://github.com/TeamCodeStream/codestream-components.git",
    "react": "^16.2.0",
    "react-dom": "^16.4.1",
    "resize-observer": "^0.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "less": "3.0.2",
    "prettier": "^1.13.7",
    "redux-mock-store": "^1.4.0",
    "rollup": "0.58.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0"
  }
}
