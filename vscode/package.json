{
    "name": "codestream",
    "version": "0.0.1",
    "author": "CodeStream",
    "publisher": "CodeStream",
    "engines": {
        "vscode": "^1.23.0"
    },
    "license": "MIT",
    "displayName": "CodeStream",
    "description": "The knowledge base behind your codebase",
    "categories": [
        "Other"
    ],
    "keywords": [
        "chat",
        "comment",
        "collaborate"
    ],
    "galleryBanner": {
        "color": "#009aef",
        "theme": "dark"
    },
    "icon": "assets/images/codestream.png",
    "preview": true,
    "homepage": "https://codestream.com",
    "bugs": {
        "url": "https://github.com/TeamCodeStream/vscode-codestream"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TeamCodeStream/vscode-codestream"
    },
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "CodeStream configuration",
            "properties": {
                "codestream.debug": {
                    "type": "boolean",
                    "default": false,
                    "description": "Specifies debug mode",
                    "scope": "window"
                },
                "codestream.traceLevel": {
                    "type": "string",
                    "default": "debug",
                    "enum": [
                        "silent",
                        "errors",
                        "verbose",
                        "debug"
                    ],
                    "description": "Specifies how much (if any) output will be sent to the CodeStream output channel",
                    "scope": "window"
                },
                "codestream.serverUrl": {
                    "type": "string",
                    "default": "https://api.codestream.com",
                    "description": "Specifies the url to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.email": {
                    "type": "string",
                    "default": null,
                    "description": "Specifies the email address to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.password": {
                    "type": "string",
                    "default": null,
                    "description": "Specifies the password to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.teamId": {
                    "type": "string",
                    "default": null,
                    "description": "Specifies the optional team to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.explorers.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Specifies whether to show the `CodeStream` explorers",
                    "scope": "window"
                },
                "codestream.notifications": {
                    "type": "string",
                    "default": "all",
                    "enum": [
                        "all",
                        "mentions",
                        "none"
                    ],
                    "description": "Specifies when to show notifications for incoming messages",
                    "scope": "window"
                },
                "codestream.bot.enabled": {
                    "type": "boolean",
                    "default": false,
                    "description": "Specifies whether to enable the CodeStream demo bot",
                    "scope": "window"
                },
                "codestream.bot.email": {
                    "type": "string",
                    "default": null,
                    "description": "Specifies the demo bot's email address to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.bot.password": {
                    "type": "string",
                    "default": null,
                    "description": "Specifies the demo bot's password to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.bot.triggers": {
                    "type": "array",
                    "default": [],
                    "items": {
                        "type": "object",
                        "required": [
                            "message",
                            "response"
                        ],
                        "properties": {
                            "message": {
                                "type": "string",
                                "description": "Specifies the demo bot's trigger message regular expression"
                            },
                            "response": {
                                "type": "object",
                                "required": [
                                    "message",
                                    "location"
                                ],
                                "properties": {
                                    "message": {
                                        "type": "string",
                                        "description": "Specifies the demo bot's response message"
                                    },
                                    "location": {
                                        "type": "string",
                                        "default": "channel",
                                        "enum": [
                                            "channel",
                                            "thread"
                                        ],
                                        "description": "Specifies where the demo bot's response should be posted"
                                    }
                                },
                                "description": "Specifies the demo bot's response"
                            }
                        }
                    },
                    "uniqueItems": true,
                    "description": "Specifies the demo bot's triggers",
                    "scope": "window"
                }
            }
        },
        "commands": [
            {
                "command": "codestream.signIn",
                "title": "Sign In...",
                "category": "CodeStream"
            },
            {
                "command": "codestream.signOut",
                "title": "Sign Out",
                "category": "CodeStream"
            },
            {
                "command": "codestream.addChannel",
                "title": "Add Channel",
                "category": "CodeStream",
                "icon": {
                    "dark": "assets/images/dark/add.svg",
                    "light": "assets/images/light/add.svg"
                }
            },
            {
                "command": "codestream.comparePostFileRevisionWithWorking",
                "title": "Compare Revision with Working",
                "category": "CodeStream"
            },
            {
                "command": "codestream.openPostWorkingFile",
                "title": "Open Working File",
                "category": "CodeStream"
            },
            {
                "command": "codestream.openPostFileRevision",
                "title": "Open File Revision",
                "category": "CodeStream"
            },
            {
                "command": "codestream.openStream",
                "title": "Open Stream",
                "category": "CodeStream"
            },
            {
                "command": "codestream.post",
                "title": "Post Message",
                "category": "CodeStream"
            },
            {
                "command": "codestream.postCode",
                "title": "Add CodeStream Comment",
                "category": "CodeStream"
            },
            {
                "command": "codestream.reset",
                "title": "Reset",
                "category": "CodeStream"
            },
            {
                "command": "codestream.show",
                "title": "Show CodeStream",
                "category": "CodeStream"
            },
            {
                "command": "codestream.toggle",
                "title": "Toggle CodeStream",
                "category": "CodeStream"
            },
            {
                "command": "codestream.vsls.invite",
                "title": "Invite to Live Share",
                "category": "CodeStream"
            },
            {
                "command": "codestream.vsls.join",
                "title": "Join Live Share",
                "category": "CodeStream"
            },
            {
                "command": "codestream.channels.hideStream",
                "title": "Close",
                "category": "CodeStream",
                "icon": {
                    "dark": "assets/images/dark/close-small.svg",
                    "light": "assets/images/light/close-small.svg"
                }
            },
            {
                "command": "codestream.people.hideStream",
                "title": "Close",
                "category": "CodeStream",
                "icon": {
                    "dark": "assets/images/dark/close-small.svg",
                    "light": "assets/images/light/close-small.svg"
                }
            },
            {
                "command": "codestream.repositories.hideStream",
                "title": "Close",
                "category": "CodeStream",
                "icon": {
                    "dark": "assets/images/dark/close-small.svg",
                    "light": "assets/images/light/close-small.svg"
                }
            },
            {
                "command": "codestream.liveshare.hideStream",
                "title": "Close",
                "category": "CodeStream",
                "icon": {
                    "dark": "assets/images/dark/close-small.svg",
                    "light": "assets/images/light/close-small.svg"
                }
            },
            {
                "command": "codestream.channels.refresh",
                "title": "Refresh",
                "category": "CodeStream",
                "icon": {
                    "dark": "assets/images/dark/refresh.svg",
                    "light": "assets/images/light/refresh.svg"
                }
            },
            {
                "command": "codestream.people.refresh",
                "title": "Refresh",
                "category": "CodeStream",
                "icon": {
                    "dark": "assets/images/dark/refresh.svg",
                    "light": "assets/images/light/refresh.svg"
                }
            },
            {
                "command": "codestream.repositories.refresh",
                "title": "Refresh",
                "category": "CodeStream",
                "icon": {
                    "dark": "assets/images/dark/refresh.svg",
                    "light": "assets/images/light/refresh.svg"
                }
            },
            {
                "command": "codestream.liveshare.refresh",
                "title": "Refresh",
                "category": "CodeStream",
                "icon": {
                    "dark": "assets/images/dark/refresh.svg",
                    "light": "assets/images/light/refresh.svg"
                }
            },
            {
                "command": "codestream.channels.refreshNode",
                "title": "Refresh",
                "category": "CodeStream"
            },
            {
                "command": "codestream.people.refreshNode",
                "title": "Refresh",
                "category": "CodeStream"
            },
            {
                "command": "codestream.repositories.refreshNode",
                "title": "Refresh",
                "category": "CodeStream"
            },
            {
                "command": "codestream.liveshare.refreshNode",
                "title": "Refresh",
                "category": "CodeStream"
            },
            {
                "command": "codestream.runServiceAction",
                "title": "Run Service Action",
                "category": "CodeStream"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "codestream.signIn",
                    "when": "codestream:status == signedOut"
                },
                {
                    "command": "codestream.signOut",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.addChannel",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.comparePostFileRevisionWithWorking",
                    "when": "false"
                },
                {
                    "command": "codestream.openPostWorkingFile",
                    "when": "false"
                },
                {
                    "command": "codestream.openPostFileRevision",
                    "when": "false"
                },
                {
                    "command": "codestream.openStream",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.post",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.postCode",
                    "when": "editorTextFocus && editorHasSelection && codestream:status == signedIn"
                },
                {
                    "command": "codestream.reset",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.show",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.toggle",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.vsls.invite",
                    "when": "false"
                },
                {
                    "command": "codestream.vsls.join",
                    "when": "false"
                },
                {
                    "command": "codestream.channels.hideStream",
                    "when": "false"
                },
                {
                    "command": "codestream.people.hideStream",
                    "when": "false"
                },
                {
                    "command": "codestream.repositories.hideStream",
                    "when": "false"
                },
                {
                    "command": "codestream.liveshare.hideStream",
                    "when": "false"
                },
                {
                    "command": "codestream.channels.refresh",
                    "when": "false"
                },
                {
                    "command": "codestream.people.refresh",
                    "when": "false"
                },
                {
                    "command": "codestream.repositories.refresh",
                    "when": "false"
                },
                {
                    "command": "codestream.liveshare.refresh",
                    "when": "false"
                },
                {
                    "command": "codestream.channels.refreshNode",
                    "when": "false"
                },
                {
                    "command": "codestream.people.refreshNode",
                    "when": "false"
                },
                {
                    "command": "codestream.repositories.refreshNode",
                    "when": "false"
                },
                {
                    "command": "codestream.liveshare.refreshNode",
                    "when": "false"
                },
                {
                    "command": "codestream.runServiceAction",
                    "when": "false"
                }
            ],
            "editor/context": [
                {
                    "command": "codestream.postCode",
                    "when": "editorTextFocus && editorHasSelection && codestream:status == signedIn",
                    "group": "1_codestream@1"
                }
            ],
            "view/title": [
                {
                    "command": "codestream.channels.refresh",
                    "when": "view == codestream.channels",
                    "group": "navigation@99"
                },
                {
                    "command": "codestream.addChannel",
                    "when": "view == codestream.channels",
                    "group": "navigation@1"
                },
                {
                    "command": "codestream.people.refresh",
                    "when": "view == codestream.people",
                    "group": "navigation@99"
                },
                {
                    "command": "codestream.repositories.refresh",
                    "when": "view == codestream.repositories",
                    "group": "navigation@99"
                },
                {
                    "command": "codestream.liveshare.refresh",
                    "when": "view == codestream.liveshare",
                    "group": "navigation@99"
                }
            ],
            "view/item/context": [
                {
                    "command": "codestream.channels.hideStream",
                    "when": "view == codestream.channels && viewItem =~ /codestream:(channel|directMessage|fileStream)\\b/",
                    "group": "inline@1"
                },
                {
                    "command": "codestream.people.hideStream",
                    "when": "view == codestream.people && viewItem =~ /codestream:(channel|directMessage|fileStream)\\b/",
                    "group": "inline@1"
                },
                {
                    "command": "codestream.repositories.hideStream",
                    "when": "view == codestream.repositories && viewItem =~ /codestream:(channel|directMessage|fileStream)\\b/",
                    "group": "inline@1"
                },
                {
                    "command": "codestream.liveshare.hideStream",
                    "when": "view == codestream.liveshare && viewItem =~ /codestream:(channel|directMessage|fileStream)\\b/",
                    "group": "inline@1"
                },
                {
                    "command": "codestream.channels.refreshNode",
                    "when": "view == codestream.channels",
                    "group": "9_codestream@1"
                },
                {
                    "command": "codestream.people.refreshNode",
                    "when": "view == codestream.people",
                    "group": "9_codestream@1"
                },
                {
                    "command": "codestream.repositories.refreshNode",
                    "when": "view == codestream.repositories",
                    "group": "9_codestream@1"
                },
                {
                    "command": "codestream.liveshare.refreshNode",
                    "when": "view == codestream.liveshare",
                    "group": "9_codestream@1"
                },
                {
                    "command": "codestream.vsls.invite",
                    "when": "codestream:liveShareInstalled && view == codestream.people && viewItem =~ /codestream:user\\b(?!:)/",
                    "group": "2_codestream@1"
                }
            ]
        },
        "views": {
            "codestream": [
                {
                    "id": "codestream.channels",
                    "name": "Channels",
                    "when": "codestream:status == signedIn && config.codestream.explorers.enabled"
                },
                {
                    "id": "codestream.people",
                    "name": "People",
                    "when": "codestream:status == signedIn && config.codestream.explorers.enabled"
                },
                {
                    "id": "codestream.repositories",
                    "name": "Repositories",
                    "when": "codestream:status == signedIn && config.codestream.explorers.enabled && false"
                },
                {
                    "id": "codestream.liveshare",
                    "name": "Live Share Sessions",
                    "when": "codestream:status == signedIn && config.codestream.explorers.enabled"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "codestream",
                    "title": "CodeStream",
                    "icon": "assets/images/codestream.svg"
                }
            ]
        }
    },
    "activationEvents": [
        "*"
    ],
    "scripts": {
        "build": "npm run compile && npm run lint",
        "compile": "tsc -p ./",
        "lint": "tslint --project tsconfig.json",
        "watch": "tsc -w -p ./",
        "pack": "vsce package",
        "webview-bundle": "pushd \"./assets/client\" && npm install --no-save && npm run vscode-bundle && popd",
        "webview-build-css": "pushd \"./assets/client\" && npm run vscode-build-css && popd",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "vscode:prepublish": "npm run build"
    },
    "dependencies": {
        "date-fns": "1.29.0",
        "lodash.debounce": "4.0.8",
        "pubnub": "4.20.3",
        "node-fetch": "2.1.2",
        "reflect-metadata": "0.1.12",
        "tmp": "0.0.33",
        "tslib": "1.9.0"
    },
    "devDependencies": {
        "@types/node": "10.0.2",
        "@types/node-fetch": "1.6.9",
        "@types/pubnub": "4.0.2",
        "@types/tmp": "0.0.33",
        "tslint": "5.9.1",
        "typescript": "2.8.3",
        "vscode": "1.1.17"
    }
}
