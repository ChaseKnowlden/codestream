{
    "name": "codestream",
    "displayName": "CodeStream",
    "description": "Discuss, review, and share code with your team. Integrates w/ Slack, Jira, Trello, and Live Share",
    "version": "0.32.3",
    "author": "CodeStream",
    "publisher": "CodeStream",
    "license": "UNLICENSED",
    "homepage": "https://codestream.com",
    "bugs": {
        "url": "https://github.com/TeamCodeStream/CodeStream/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TeamCodeStream/CodeStream"
    },
    "engines": {
        "vscode": "^1.29.0"
    },
    "main": "./dist/extension",
    "icon": "assets/images/codestream.png",
    "preview": true,
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#eff1f3",
        "theme": "light"
    },
    "keywords": [
        "chat",
        "slack",
        "comment",
        "collaboration",
        "live"
    ],
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "CodeStream configuration",
            "properties": {
                "codestream.autoSignIn": {
                    "type": "boolean",
                    "default": true,
                    "description": "Specifies whether to automatically sign in to CodeStream",
                    "scope": "window"
                },
                "codestream.avatars": {
                    "type": "boolean",
                    "default": true,
                    "description": "Specifies whether to show avatars",
                    "scope": "window"
                },
                "codestream.email": {
                    "type": "string",
                    "default": null,
                    "description": "Specifies the email address to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.notifications": {
                    "type": "string",
                    "default": "mentions",
                    "enum": [
                        "all",
                        "mentions",
                        "none"
                    ],
                    "enumDescriptions": [
                        "Shows notifications for all incoming messages",
                        "Shows notifications for all direct messages and @mentions",
                        "No notifications will be shown"
                    ],
                    "description": "Specifies when to show notifications for incoming messages",
                    "scope": "window"
                },
                "codestream.serverUrl": {
                    "type": "string",
                    "default": "https://api.codestream.com",
                    "description": "Specifies the url to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.showInStatusBar": {
                    "type": "string",
                    "enum": [
                        "left",
                        "right",
                        false
                    ],
                    "enumDescriptions": [
                        "Shows the CodeStream status bar item on the left",
                        "Shows the CodeStream status bar item on the right",
                        "No CodeStream status bar item will be shown"
                    ],
                    "default": "left",
                    "description": "Specifies whether to show CodeStream in the status bar",
                    "scope": "window"
                },
                "codestream.showMarkers": {
                    "type": "boolean",
                    "default": true,
                    "description": "Specifies whether to show CodeStream markers in editor buffers"
                },
                "codestream.muteAll": {
                    "type": "boolean",
                    "default": false,
                    "description": "Specifies whether to indicate when new messages arrive"
                },
                "codestream.openCommentOnSelect": {
                    "type": "boolean",
                    "default": true,
                    "description": "Specifies whether to automatically open the comment dialog when the CodeStream panel is open and you select code"
                },
                "codestream.team": {
                    "type": "string",
                    "default": null,
                    "description": "Specifies an optional team to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.traceLevel": {
                    "type": "string",
                    "default": "verbose",
                    "enum": [
                        "silent",
                        "errors",
                        "verbose",
                        "debug"
                    ],
                    "description": "Specifies how much (if any) output will be sent to the CodeStream output channel",
                    "scope": "window"
                },
                "codestream.webAppUrl": {
                    "type": "string",
                    "default": "https://app.codestream.com",
                    "description": "Specifies the url for the CodeStream web portal",
                    "scope": "window"
                }
            }
        },
        "commands": [
            {
                "command": "codestream.goOffline",
                "title": "Go Offline",
                "category": "CodeStream"
            },
            {
                "command": "codestream.signIn",
                "title": "Sign In...",
                "category": "CodeStream"
            },
            {
                "command": "codestream.signOut",
                "title": "Sign Out",
                "category": "CodeStream"
            },
            {
                "command": "codestream.postCode",
                "title": "Add CodeStream Comment",
                "category": "CodeStream"
            },
            {
                "command": "codestream.toggle",
                "title": "Toggle CodeStream",
                "category": "CodeStream"
            }
        ],
        "keybindings": [
            {
                "command": "codestream.postCode",
                "key": "cmd+/ cmd+/",
                "mac": "ctrl+/ ctrl+/",
                "when": "editorTextFocus && codestream:status == signedIn"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "codestream.goOffline",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.signIn",
                    "when": "codestream:status != signedIn && codestream:status != signingIn"
                },
                {
                    "command": "codestream.signOut",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.postCode",
                    "when": "editorTextFocus && editorHasSelection && codestream:status == signedIn"
                },
                {
                    "command": "codestream.toggle",
                    "when": "codestream:status == signedIn"
                }
            ],
            "editor/context": [
                {
                    "command": "codestream.postCode",
                    "when": "editorTextFocus && editorHasSelection && codestream:status == signedIn",
                    "group": "1_codestream@1"
                }
            ]
        }
    },
    "activationEvents": [
        "*"
    ],
    "scripts": {
        "build": "webpack --env.development",
        "bundle": "npm install --no-save && npm run agent:bundle && webpack --env.production",
        "bundle:ci": "npm run clean:ci && npm run bundle",
        "clean": "git clean -Xdf -e !node_modules -e !node_modules/**/* -e !codestream*.info",
        "clean:ci": "git clean -xdf && npm run webview:clean:ci && npm run agent:clean:ci",
        "lint": "tslint --project tsconfig.json",
        "pack": "vsce package",
        "pretty": "prettier --config .prettierrc --write \"./src/**/*.ts\" \"./src/**/*.js\" ./package.json ./.prettierrc",
        "rebuild": "npm run clean && npm install --no-save && npm run agent:rebuild && npm run build",
        "test": "npm run agent:test",
        "watch": "webpack --watch --env.development --info-verbosity verbose",
        "agent:build": "pushd \"../codestream-lsp-agent\" && npm run build && popd",
        "agent:bundle": "pushd \"../codestream-lsp-agent\" && npm run bundle && popd",
        "agent:bundle:ci": "pushd \"../codestream-lsp-agent\" && npm run bundle:ci && popd",
        "agent:clean:ci": "pushd \"../codestream-lsp-agent\" && npm run clean:ci && popd",
        "agent:rebuild": "pushd \"../codestream-lsp-agent\" && npm run rebuild && popd",
        "agent:test": "pushd \"../codestream-lsp-agent\" && npm run test && popd",
        "agent:watch": "pushd \"../codestream-lsp-agent\" && npm run watch && popd",
        "webview:clean:ci": "pushd \"../codestream-components\" && npm run clean:ci && popd",
        "webview:watch": "webpack --watch --config-name webview --env.development --info-verbosity verbose",
        "preinstall": "pushd \"../codestream-components\" && npm install --no-save && popd",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "date-fns": "1.30.1",
        "lodash-es": "4.17.11",
        "tslib": "1.9.3",
        "uuid": "^3.3.2",
        "vscode-languageclient": "5.2.1",
        "vsls": "0.3.967"
    },
    "devDependencies": {
        "@types/keytar": "4.0.1",
        "@types/lodash-es": "4.17.1",
        "@types/node": "8.10.39",
        "@types/node-fetch": "2.1.2",
        "@types/pubnub": "4.0.4",
        "@types/uuid": "^3.4.4",
        "babel-core": "6.26.3",
        "babel-loader": "7.1.5",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-plugin-transform-object-rest-spread": "6.26.0",
        "babel-preset-env": "1.7.0",
        "babel-preset-react": "6.24.1",
        "clean-webpack-plugin": "1.0.1",
        "css-loader": "2.1.0",
        "filemanager-webpack-plugin": "2.0.5",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "less": "3.9.0",
        "less-loader": "4.1.0",
        "mini-css-extract-plugin": "0.5.0",
        "prettier": "1.16.1",
        "prettier-tslint": "0.4.2",
        "react": "16.6.3",
        "react-dom": "16.6.3",
        "redux-mock-store": "1.5.3",
        "resize-observer": "1.0.0",
        "terser-webpack-plugin": "1.2.1",
        "ts-loader": "5.3.3",
        "tslint": "5.12.1",
        "tslint-loader": "3.5.4",
        "typescript": "3.2.4",
        "vsce": "1.54.0",
        "vscode": "1.1.27",
        "webpack": "4.29.0",
        "webpack-cli": "3.2.1"
    }
}
