{
    "name": "codestream",
    "displayName": "CodeStream",
    "description": "Discuss, review, and share code with your team. Integrates w/ Slack, Jira, Trello, and Live Share",
    "version": "0.35.1",
    "author": "CodeStream",
    "publisher": "CodeStream",
    "license": "UNLICENSED",
    "homepage": "https://codestream.com",
    "bugs": {
        "url": "https://github.com/TeamCodeStream/CodeStream/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TeamCodeStream/CodeStream"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "main": "./dist/extension",
    "icon": "assets/images/codestream.png",
    "preview": true,
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#eff1f3",
        "theme": "light"
    },
    "keywords": [
        "chat",
        "slack",
        "comment",
        "collaboration",
        "live"
    ],
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "CodeStream",
            "properties": {
                "codestream.autoSignIn": {
                    "type": "boolean",
                    "default": true,
                    "description": "Specifies whether to automatically sign in to CodeStream",
                    "scope": "window"
                },
                "codestream.avatars": {
                    "type": "boolean",
                    "default": true,
                    "description": "Specifies whether to show avatars",
                    "scope": "window"
                },
                "codestream.email": {
                    "type": "string",
                    "default": null,
                    "description": "Specifies the email address to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.markerStyle": {
                    "type": "string",
                    "default": "glyphs",
                    "enum": [
                        "glyphs",
                        "codelens"
                    ],
                    "enumDescriptions": [
                        "Adds glyph indicators at the start of lines with associate codemarks",
                        "Adds code lens above lines with associate codemarks"
                    ],
                    "description": "Specifies the style of CodeStream codemarks shown in the editor",
                    "scope": "window"
                },
                "codestream.muteAll": {
                    "type": "boolean",
                    "default": false,
                    "description": "Specifies whether to show indicators when new messages arrive"
                },
                "codestream.notifications": {
                    "type": "string",
                    "default": "mentions",
                    "enum": [
                        "all",
                        "mentions",
                        "none"
                    ],
                    "enumDescriptions": [
                        "Shows notifications for all incoming messages",
                        "Shows notifications for all direct messages and @mentions",
                        "No notifications will be shown"
                    ],
                    "description": "Specifies when to show notifications for incoming messages",
                    "scope": "window"
                },
                "codestream.serverUrl": {
                    "type": "string",
                    "default": "https://api.codestream.com",
                    "description": "Specifies the url to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.showInStatusBar": {
                    "type": "string",
                    "enum": [
                        "left",
                        "right",
                        false
                    ],
                    "enumDescriptions": [
                        "Shows the CodeStream status bar item on the left",
                        "Shows the CodeStream status bar item on the right",
                        "No CodeStream status bar item will be shown"
                    ],
                    "default": "left",
                    "description": "Specifies whether to show CodeStream in the status bar",
                    "scope": "window"
                },
                "codestream.showMarkers": {
                    "type": "boolean",
                    "default": true,
                    "description": "Specifies whether to show CodeStream codemarks in the editor"
                },
                "codestream.showFeedbackSmiley": {
                    "type": "boolean",
                    "default": true,
                    "description": "Specifies whether to show a feedback button in the CodeStream panel"
                },
                "codestream.showShortcutTipOnSelection": {
                    "type": "boolean",
                    "default": false,
                    "description": "Specifies whether to show a shortcut key tip attached to the selection when you select code"
                },
                "codestream.team": {
                    "type": "string",
                    "default": null,
                    "description": "Specifies an optional team to use to connect to the CodeStream service",
                    "scope": "window"
                },
                "codestream.traceLevel": {
                    "type": "string",
                    "default": "verbose",
                    "enum": [
                        "silent",
                        "errors",
                        "verbose",
                        "debug"
                    ],
                    "description": "Specifies how much (if any) output will be sent to the CodeStream output channel",
                    "scope": "window"
                },
                "codestream.viewCodemarksInline": {
                    "type": "boolean",
                    "default": true,
                    "description": "Specifies whether to display Codemarks inline next to the code they refer to. The alternative is a list view."
                },
                "codestream.webAppUrl": {
                    "type": "string",
                    "default": "https://app.codestream.com",
                    "description": "Specifies the url for the CodeStream web portal",
                    "scope": "window"
                }
            }
        },
        "commands": [
            {
                "command": "codestream.goOffline",
                "title": "Go Offline",
                "category": "CodeStream"
            },
            {
                "command": "codestream.signIn",
                "title": "Sign In...",
                "category": "CodeStream"
            },
            {
                "command": "codestream.signOut",
                "title": "Sign Out",
                "category": "CodeStream"
            },
            {
                "command": "codestream.newComment",
                "title": "Add Comment",
                "category": "CodeStream"
            },
            {
                "command": "codestream.newIssue",
                "title": "Create Issue",
                "category": "CodeStream"
            },
            {
                "command": "codestream.newBookmark",
                "title": "Create Bookmark",
                "category": "CodeStream"
            },
            {
                "command": "codestream.newPermalink",
                "title": "Get Permalink",
                "category": "CodeStream"
            },
            {
                "command": "codestream.copyPermalink",
                "title": "Copy Permalink to Clipboard",
                "category": "CodeStream"
            },
            {
                "command": "codestream.toggle",
                "title": "Toggle CodeStream",
                "category": "CodeStream"
            }
        ],
        "keybindings": [
            {
                "command": "codestream.newComment",
                "args": {
                    "source": "Shortcut"
                },
                "key": "ctrl+shift+/ ctrl+shift+/",
                "mac": "ctrl+/ ctrl+/",
                "when": "editorTextFocus && codestream:status == signedIn"
            },
            {
                "command": "codestream.newComment",
                "args": {
                    "source": "Shortcut"
                },
                "key": "ctrl+shift+/ c",
                "mac": "ctrl+/ c",
                "when": "editorTextFocus && codestream:status == signedIn"
            },
            {
                "command": "codestream.newIssue",
                "args": {
                    "source": "Shortcut"
                },
                "key": "ctrl+shift+/ i",
                "mac": "ctrl+/ i",
                "when": "editorTextFocus && codestream:status == signedIn"
            },
            {
                "command": "codestream.newBookmark",
                "args": {
                    "source": "Shortcut"
                },
                "key": "ctrl+shift+/ b",
                "mac": "ctrl+/ b",
                "when": "editorTextFocus && codestream:status == signedIn"
            },
            {
                "command": "codestream.newPermalink",
                "args": { "source": "Shortcut" },
                "key": "ctrl+shift+/ p",
                "mac": "ctrl+/ p",
                "when": "editorTextFocus && codestream:status == signedIn"
            },
            {
                "command": "codestream.copyPermalink",
                "args": {
                    "source": "Shortcut"
                },
                "key": "ctrl+shift+/ shift+p",
                "mac": "ctrl+/ shift+p",
                "when": "editorTextFocus && codestream:status == signedIn"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "codestream.goOffline",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.signIn",
                    "when": "codestream:status != signedIn && codestream:status != signingIn"
                },
                {
                    "command": "codestream.signOut",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.newComment",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.newIssue",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.newBookmark",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.newPermalink",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.copyPermalink",
                    "when": "codestream:status == signedIn"
                },
                {
                    "command": "codestream.toggle",
                    "when": "codestream:status == signedIn"
                }
            ],
            "editor/context": [
                {
                    "command": "codestream.newComment",
                    "when": "codestream:status == signedIn",
                    "group": "9_zcodestream@1"
                },
                {
                    "command": "codestream.newIssue",
                    "when": "codestream:status == signedIn",
                    "group": "9_zcodestream@2"
                },
                {
                    "command": "codestream.newBookmark",
                    "when": "codestream:status == signedIn",
                    "group": "9_zcodestream@3"
                },
                {
                    "command": "codestream.newPermalink",
                    "when": "codestream:status == signedIn",
                    "group": "9_zcodestream@4"
                }
            ]
        },
        "views": {
            "codestream": [
                {
                    "id": "codestream",
                    "name": "CodeStream"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "codestream",
                    "title": "CodeStream",
                    "icon": "assets/images/codestream.svg"
                }
            ]
        }
    },
    "activationEvents": [
        "*"
    ],
    "scripts": {
        "analyze:bundle": "webpack --env.analyzeBundle",
        "analyze:deps": "webpack --env.analyzeDeps",
        "build": "webpack --env.development",
        "bundle": "npm install --no-save && npm run agent:bundle && webpack --env.production",
        "bundle:ci": "npm run clean:ci && npm run bundle",
        "clean": "git clean -Xdf -e !node_modules -e !node_modules/**/* -e !codestream*.info",
        "clean:ci": "git clean -xdf && npm run webview:clean:ci && npm run agent:clean:ci",
        "lint": "tslint --project tsconfig.json",
        "pack": "vsce package",
        "pretty": "prettier --config .prettierrc --write \"./src/**/*.ts\" \"./src/**/*.js\" ./package.json ./.prettierrc",
        "rebuild": "npm run clean && npm install --no-save && npm run agent:rebuild && npm run build",
        "test": "npm run agent:test",
        "watch": "webpack --watch --env.development --info-verbosity verbose",
        "agent:build": "pushd \"../codestream-lsp-agent\" && npm run build && popd",
        "agent:bundle": "pushd \"../codestream-lsp-agent\" && npm run bundle && popd",
        "agent:bundle:ci": "pushd \"../codestream-lsp-agent\" && npm run bundle:ci && popd",
        "agent:clean:ci": "pushd \"../codestream-lsp-agent\" && npm run clean:ci && popd",
        "agent:rebuild": "pushd \"../codestream-lsp-agent\" && npm run rebuild && popd",
        "agent:test": "pushd \"../codestream-lsp-agent\" && npm run test && popd",
        "agent:watch": "pushd \"../codestream-lsp-agent\" && npm run watch && popd",
        "webview:analyze": "webpack --env.analyzeBundleWebview",
        "webview:build": "webpack --config-name webview --env.development",
        "webview:clean:ci": "pushd \"../codestream-components\" && npm run clean:ci && popd",
        "webview:watch": "webpack --watch --config-name webview --env.development --info-verbosity verbose",
        "preinstall": "pushd \"../codestream-components\" && npm install --no-save && popd",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "date-fns": "1.30.1",
        "lodash-es": "4.17.11",
        "uuid": "3.3.2",
        "vscode-languageclient": "5.2.1",
        "vsls": "0.3.1291"
    },
    "devDependencies": {
        "@types/keytar": "4.0.1",
        "@types/lodash-es": "4.17.3",
        "@types/node": "10.1.4",
        "@types/node-fetch": "2.1.6",
        "@types/uuid": "3.4.4",
        "@babel/core": "7.3.4",
        "@babel/cli": "7.2.3",
        "@babel/plugin-proposal-class-properties": "7.3.4",
        "@babel/plugin-proposal-object-rest-spread": "7.3.4",
        "@babel/plugin-syntax-dynamic-import": "7.2.0",
        "@babel/plugin-transform-runtime": "7.3.4",
        "@babel/preset-env": "7.3.4",
        "@babel/preset-react": "7.0.0",
        "@babel/preset-typescript": "7.3.3",
        "@babel/runtime": "7.3.4",
        "babel-loader": "8.0.5",
        "circular-dependency-plugin": "5.0.2",
        "clean-webpack-plugin": "1.0.1",
        "css-loader": "2.1.1",
        "filemanager-webpack-plugin": "2.0.5",
        "fork-ts-checker-webpack-plugin": "1.0.0",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "less": "3.9.0",
        "less-loader": "4.1.0",
        "mini-css-extract-plugin": "0.5.0",
        "prettier": "1.16.4",
        "prettier-tslint": "0.4.2",
        "react": "16.6.3",
        "react-dom": "16.6.3",
        "redux-mock-store": "1.5.3",
        "terser-webpack-plugin": "1.2.3",
        "ts-loader": "5.3.3",
        "tslint": "5.13.1",
        "tslint-loader": "3.5.4",
        "tsconfig-paths-webpack-plugin": "3.2.0",
        "typescript": "3.2.4",
        "vsce": "1.57.1",
        "vscode": "1.1.30",
        "webpack": "4.29.6",
        "webpack-bundle-analyzer": "3.1.0",
        "webpack-cli": "3.2.3"
    }
}
